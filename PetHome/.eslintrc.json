{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["warn", "double"],
    "semi": ["off"],
    "no-unused-vars": ["warn"]
  }
}
