{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "quotes": ["warn", "double"],
    "semi": ["off"],
    "no-unused-vars": ["warn"],
    "react/prop-types": ["off"]
  }
}
